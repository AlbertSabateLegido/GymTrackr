<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gymtrackr">

    <application
        android:name=".GymTrackr"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AssignExercisesActivity" />
        <activity android:name=".AddRoutineActivity" />
        <activity android:name=".ShowRoutineActivity" />
        <activity android:name=".AddExerciseActivity" />
        <activity android:name=".EditNameActivity" />
        <activity android:name=".EditAssignedExercisesActivity" />
        <activity android:name=".RealizeRoutineActivity" />
        <activity android:name=".RealizeExercise" />
        <activity android:name=".ShowExerciseActivity">
        </activity>
    </application>

</manifest>